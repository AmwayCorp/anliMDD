#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_custom-react-scripts@0.2.2@custom-react-scripts/bin/react-scripts.js" "$@"
  ret=$?
else 
  node  "$basedir/../_custom-react-scripts@0.2.2@custom-react-scripts/bin/react-scripts.js" "$@"
  ret=$?
fi
exit $ret
